{
    "name": "jobverplanke/action-test",
    "type": "project",
    "description": "Personal homepage",
    "keywords": ["personal", "homepage", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "ext-sockets" : "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Verplanke\\Ikea\\": "src/Ikea/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear": [
            "@php artisan config:clear",
            "@php artisan cache:clear",
            "@php artisan event:clear",
            "@php artisan route:clear",
            "@php artisan view:clear"
        ],
        "lint": "php-cs-fixer fix",
        "test:types": "./vendor/bin/phpstan analyse --ansi --memory-limit=-1",
        "test:unit": "phpunit --colors=always",
        "test:lint": "php-cs-fixer fix -v --show-progress=none --dry-run",
        "test": [
            "@test:types",
            "@test:unit",
            "@test:lint"
        ],
        "coverage": "@test:unit --coverage-html docs/coverage"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
